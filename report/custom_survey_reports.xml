<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="action_certification_report" model="ir.actions.report">
            <field name="name">Certification FNA</field>
            <field name="model">survey.user_input</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">survey.certification_report_view</field>
            <field name="print_report_name">'Certification - %s - %s - %s' % (object.survey_id.display_name, object.partner_id.name, object.create_date.strftime('%Y-%m-%d')) </field>
            <field name="binding_model_id" ref="survey.model_survey_user_input"/>
            <field name="binding_type">report</field>
        </record>
    </data>
</odoo>
